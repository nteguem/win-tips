          "max": 5,
          "step": 0.5,
          "default": 1.5
        },
        {
          "name": "direction",
          "type": "enum",
          "label": {
            "fr": "Plus ou moins",
            "en": "Over or under"
          },
          "options": [
            {
              "value": "over",
              "label": {
                "fr": "Plus",
                "en": "Over"
              }
            },
            {
              "value": "under",
              "label": {
                "fr": "Moins",
                "en": "Under"
              }
            }
          ],
          "default": "over"
        }
      ]
    },
    {
      "id": "home_goals",
      "position": 3,
      "priority": "medium",
      "labelTemplate": {
        "fr": "Domicile {{direction}} {{value}} buts",
        "en": "Home {{direction}} {{value}} goals"
      },
      "expressionTemplate": "score.home {{operator}} {{value}}",
      "category": "team_goals",
      "description": {
        "fr": "Prédiction sur le nombre de buts de l'équipe domicile",
        "en": "Prediction on the number of goals by home team"
      },
      "paramFields": [
        {
          "name": "value",
          "type": "number",
          "label": {
            "fr": "Nombre de buts",
            "en": "Number of goals"
          },
          "min": 0,
          "max": 5,
          "step": 0.5,
          "default": 1.5
        },
        {
          "name": "direction",
          "type": "enum",
          "label": {
            "fr": "Plus ou moins",
            "en": "Over or under"
          },
          "options": [
            {
              "value": "over",
              "label": {
